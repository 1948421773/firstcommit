<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/register_code_error"
    >
    <RelativeLayout
        android:id="@+id/rl_setting_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="@color/register_code_error">
        <RelativeLayout
            android:id="@+id/rl_top"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center_vertical"
            android:background="@color/white"
            >

            <LinearLayout
                android:id="@+id/ll_back"
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:gravity="left"
                android:layout_gravity="center"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/iv_back"
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    android:layout_marginLeft="8dp"
                    android:src="@mipmap/back"/>
            </LinearLayout>
            <TextView
                android:id="@+id/tv_title"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="设置"
                android:textColor="@color/text_black"
                android:textSize="20sp"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true" />
            <RelativeLayout
                android:id="@id/rl_payment_way"
                android:layout_width="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_height="match_parent"
                ></RelativeLayout>
        </RelativeLayout>
        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/rl_top"
            android:background="@color/register_code_error"/>
        <RelativeLayout
            android:id="@+id/rl_account_security"
            android:layout_below="@id/view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:background="@color/white"
            >
            <TextView
                android:id="@+id/tv_account_security"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/buttom_white_bg"
                android:text="账户安全"
                android:ellipsize="end"
                android:singleLine="true"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"
                android:textSize="18dp"
                android:maxLength="20"
                android:paddingLeft="30dp"
                android:paddingRight="60dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:background="@mipmap/home_more_icon"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:visibility="gone"
                android:layout_below="@id/tv_account_security"
                android:background="@color/shoppingcart_unaccount"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_message_reminder"
            android:layout_below="@id/rl_account_security"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:background="@color/white"
            >
            <TextView
                android:id="@+id/tv_message_reminder"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="通用"
                android:visibility="gone"
                android:ellipsize="end"
                android:textSize="18dp"
                android:singleLine="true"
                android:background="@drawable/buttom_white_bg"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"
                android:maxLength="20"
                android:paddingLeft="30dp"
                android:paddingRight="60dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:background="@mipmap/home_more_icon"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="6px"
                android:layout_below="@id/tv_message_reminder"
                android:background="@color/shoppingcart_line"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_account_association"
            android:layout_below="@id/rl_message_reminder"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >
            <TextView
                android:id="@+id/tv_account_association"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/buttom_white_bg"
                android:text="账户关联"
                android:ellipsize="end"
                android:textSize="18dp"
                android:singleLine="true"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"
                android:maxLength="20"
                android:paddingLeft="30dp"
                android:paddingRight="60dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:background="@mipmap/home_more_icon"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="6px"
                android:layout_below="@id/tv_account_association"
                android:background="@color/shoppingcart_line"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_clear_cace"
            android:layout_below="@id/rl_account_association"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >
            <TextView
                android:id="@+id/tv_clear_cace"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/buttom_white_bg"
                android:text="清理缓存"
                android:ellipsize="end"
                android:singleLine="true"
                android:textSize="18dp"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"
                android:maxLength="20"
                android:paddingLeft="30dp"
                android:paddingRight="60dp"/>
            <ImageView
                android:id="@+id/im_clear_size_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:background="@mipmap/home_more_icon"
                />
            <TextView
                android:id="@+id/tv_clear_size_text"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:ellipsize="end"
                android:textSize="18dp"
                android:singleLine="true"
                android:layout_marginRight="20dp"
                android:layout_toLeftOf="@id/im_clear_size_arrow"
                android:background="@drawable/buttom_white_bg"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"
                android:maxLength="20"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_below="@id/tv_clear_cace"
                android:background="@color/shoppingcart_unaccount"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_feedback"
            android:layout_below="@id/rl_clear_cace"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >
            <TextView
                android:id="@+id/tv_feedback"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/buttom_white_bg"
                android:text="功能反馈"
                android:ellipsize="end"
                android:singleLine="true"
                android:textSize="18dp"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"
                android:maxLength="20"
                android:paddingLeft="30dp"
                android:paddingRight="60dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:background="@mipmap/home_more_icon"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_below="@id/tv_feedback"
                android:background="@color/shoppingcart_unaccount"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@id/rl_feedback"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >
            <TextView
                android:id="@+id/tv_rl_aboutus"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/buttom_white_bg"
                android:text="关于我们"
                android:ellipsize="end"
                android:textSize="18dp"
                android:singleLine="true"
                android:gravity="center_vertical"
                android:layout_centerVertical="true"
                android:maxLength="20"
                android:paddingLeft="30dp"
                android:paddingRight="60dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:background="@mipmap/home_more_icon"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_below="@id/tv_rl_aboutus"
                android:background="@color/shoppingcart_unaccount"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >
            <TextView
                android:id="@+id/tv_logout"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/buttom_red_bg"
                android:text="退出登录"
                android:layout_centerHorizontal="true"
                android:ellipsize="end"
                android:textSize="18dp"
                android:singleLine="true"
                android:gravity="center"
                android:textColor="@color/white"
                android:layout_centerVertical="true"
                />

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_below="@id/tv_logout"
                android:background="@color/shoppingcart_unaccount"/>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>
